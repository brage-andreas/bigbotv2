const chalk = require("chalk");
const { botLog } = require("../files/auto.js");

// --------------------------------------------------------------

module.exports = {
	name: [""],
	use: "",
	about: "",
	category: "management|fun|info|support"
}

// --------------------------------------------------------------

module.exports.run = async (message, args) => {
    const { client } = message;

    botLog(client.user.id, chalk `{grey Used} X`);
}
